---
title: List of all posts
permalink: /api/posts
---
[
    {% for post in site.posts %}
        {
            "date": "{{ post.date | date: "%Y-%m-%d" }}",
            "title": "{{ post.title }}",
            "tags": {{ post.tags | jsonify }},
            "url": "{{ post.url }}",
            "picture"   : "{{ post.image }}",
            "country"   : "{{ post.country }}",
            "city"      : "{{ post.city }}"
        }
        {% unless forloop.last %},{% endunless %}
    {% endfor %}
] 
